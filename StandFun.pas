// Standart math functions
var x: real;

begin 
  write('Input x: ');
  readln(x);
  writeln('Sqr ',x,' = ',sqr(x));
  writeln(x,' x^5 ',power(x,5));
  writeln('Sqrt ',x,' = ',sqrt(x));
  writeln('Module ',x,' = ',abs(x));
  writeln('Ln ',x,' = ',ln(x));
  writeln('Sinus ',x,' = ',sin(x));
  writeln('Cosinus ',x,' = ',cos(x));
end.